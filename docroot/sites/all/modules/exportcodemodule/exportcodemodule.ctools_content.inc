<?php

/**
 * @file
 * Bulk export of ctools_content objects generated by Bulk export module.
 */

/**
 * Implements hook_default_ctools_custom_content().
 */
function exportcodemodule_default_ctools_custom_content() {
  $contents = array();

  $content = new stdClass();
  $content->disabled = FALSE; /* Edit this to true to make a default content disabled initially */
  $content->api_version = 1;
  $content->name = 'subtopic_title_display';
  $content->admin_title = 'Subtopic Title Display';
  $content->admin_description = '';
  $content->category = '';
  $content->settings = array(
    'admin_title' => 'Subtopic Title Display',
    'title' => '%node:title',
    'body' => '',
    'format' => 'filtered_html',
    'substitute' => 1,
  );
  $contents['subtopic_title_display'] = $content;

  $content = new stdClass();
  $content->disabled = FALSE; /* Edit this to true to make a default content disabled initially */
  $content->api_version = 1;
  $content->name = 'subtopic_title_display2';
  $content->admin_title = 'Subtopic Title Display 2';
  $content->admin_description = '';
  $content->category = '';
  $content->settings = array(
    'admin_title' => 'Subtopic Title Display 2',
    'title' => '%node:title',
    'body' => '',
    'format' => 'full_html',
    'substitute' => 1,
  );
  $contents['subtopic_title_display2'] = $content;

  return $contents;
}
